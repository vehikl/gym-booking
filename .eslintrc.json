{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": 0,
      "max-len": [1, { "code": 120 }],
      "no-shadow": 0,
      "no-plusplus": 0,
      "no-param-reassign": 0,
      "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
      "react/no-array-index-key": 0,
      "radix": 0
    }
}
